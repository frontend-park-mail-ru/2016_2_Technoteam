{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": 0,
        "class-methods-use-this": 0,
        "no-undef": 0,
        "no-unused-vars": 0,
        "no-console": 0,
        "global-require": 0,
        "no-alert": 0,
        "wrap-iife": 0,
        "func-names": 0,
        "no-underscore-dangle": 0
    }
}
